<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.jp.core.persistence.pojo.Project" table="project">
        <id name="proyectId" type="java.lang.Integer">
            <column name="proyect_id" />
            <generator class="identity" />
        </id>
        <many-to-one name="catState" class="org.jp.core.persistence.pojo.CatState" fetch="select">
            <column name="id_state" not-null="true" />
        </many-to-one>
        <property name="description" type="string">
            <column name="description" not-null="true" />
        </property>
        <property name="info" type="string">
            <column name="info" length="65535" not-null="true" />
        </property>
        <property name="dateStart" type="timestamp">
            <column name="date_start" length="0" not-null="true" />
        </property>
        <property name="logo" type="binary">
            <column name="logo" />
        </property>
        <property name="header" type="string">
            <column name="header" />
        </property>
        <property name="range" type="string">
            <column name="range" />
        </property>
        <set name="proyectLocations" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.ProjectLocation" />
        </set>
        <set name="surveyses" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.Surveys" />
        </set>
        <set name="proyectGroups" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.ProjectGroup" />
        </set>
        <set name="surveyLocations" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.SurveyLocation" />
        </set>
        <set name="proyectTypeSurveies" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.ProjectTypeSurvey" />
        </set>
        <set name="catLocationUsers" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.CatLocationUser" />
        </set>
        <set name="proyectUsers" inverse="true">
            <key>
                <column name="proyect_id" not-null="true" />
            </key>
            <one-to-many class="org.jp.core.persistence.pojo.ProjectUser" />
        </set>
    </class>
</hibernate-mapping>
