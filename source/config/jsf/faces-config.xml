<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>admon</from-outcome>
   <to-view-id>/pages/admon/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>surveys</from-outcome>
   <to-view-id>/pages/surveys.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>stats</from-outcome>
   <to-view-id>/pages/stats.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>reports</from-outcome>
   <to-view-id>/pages/reports.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>groups</from-outcome>
   <to-view-id>/pages/admon/groups.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>users</from-outcome>
   <to-view-id>/pages/admon/users.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>location</from-outcome>
   <to-view-id>/pages/admon/location.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>project</from-outcome>
   <to-view-id>/pages/admon/project.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>createSurvey</from-outcome>
   <to-view-id>/pages/createSurvey.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>organizeSurveys</from-outcome>
   <to-view-id>/pages/organizeSurveys.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>polls</from-outcome>
   <to-view-id>/pages/polls.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>tweetpoll</from-outcome>
   <to-view-id>/pages/tweetpoll.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>tweetPollCredentials</from-outcome>
   <to-view-id>/pages/tweetPollCredentials.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createTweetPoll</from-outcome>
   <to-view-id>/pages/createTweetPoll.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/admon/index.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/admon/groups.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/admon/users.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>register</from-outcome>
   <to-view-id>/register.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>forget</from-outcome>
   <to-view-id>/user/forget.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/admon/project.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/admon/location.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/createSurvey.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>buildSurvey</from-outcome>
   <to-view-id>/templates/includes/survey/buildSurvey.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
