<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from>^/tweet/([0-9]+)$</from>
        <to>/sr/tweetPollController?id=$1</to>
    </rule>
    <rule>
        <from>^/results/([0-9]+)$</from>
        <to>/sr/resultsController?id=$1</to>
    </rule>
    <rule>
        <from>^/poll/([0-9]+)$</from>
        <to>/sr/pollController?id=$1</to>
    </rule>
    <rule>
        <from>^/survey/([0-9]+)$</from>
        <to>/sr/surveyController?id=$1</to>
    </rule>
    <rule>
        <from>^/report/([0-9]+)$</from>
        <to>/sr/reportController?id=$1</to>
    </rule>
    <rule>
        <from>^/announce/([0-9]+)$</from>
        <to>/sr/announceController?id=$1</to>
    </rule>
</urlrewrite>
