{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "parser": "babel-eslint",
  "rules": {
    "eqeqeq": [2, "allow-null"],
    "func-names": 0,
    "no-param-reassign": 0,
    "new-cap": 0,
    "max-len": 0,
    "no-console": 0,
    "no-case-declarations": 0,
    "consistent-return": 0,
    "global-require": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-no-bind": 0,
    "react/sort-comp": 0
  }
}