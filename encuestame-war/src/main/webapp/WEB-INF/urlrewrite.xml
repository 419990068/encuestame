<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from>^/tweet/(.*)$</from>
        <to>/tweet/$1/vote.html</to>
    </rule>
    <rule>
        <from>^/results/(.*)$</from>
        <to>/resultsController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/poll/(.*)$</from>
        <to>/pollController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/survey/(.*)$</from>
        <to>/surveyController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/report/(.*)$</from>
        <to>/reportController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/announce/(.*)$</from>
        <to>/announceController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/user/signin</from>
        <to>/signin.html</to>
    </rule>
    <rule>
      <from>^/signin.html</from>
      <to type="redirect">user/signin</to>
    </rule>
    <rule>
      <from>^/index.html</from>
      <to type="redirect">home</to>
    </rule>
    <rule>
        <from>^/home</from>
        <to>/index.html</to>
    </rule>
    <rule>
        <from>^/user/signup</from>
        <to>/user/signup.html</to>
    </rule>
    <rule>
        <from>^/user/forgot</from>
        <to>/user/forgot.html</to>
    </rule>
</urlrewrite>
