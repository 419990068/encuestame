<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from>^/tweet/(.*)$</from>
        <to>/tweetPollController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/results/(.*)$</from>
        <to>/resultsController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/poll/(.*)$</from>
        <to>/pollController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/survey/(.*)$</from>
        <to>/surveyController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/report/(.*)$</from>
        <to>/reportController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/announce/(.*)$</from>
        <to>/announceController.do?id=$1</to>
    </rule>
    <rule>
        <from>^/signup</from>
        <to>/register.do</to>
    </rule>
</urlrewrite>
