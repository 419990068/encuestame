<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:security="http://www.springframework.org/schema/security"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:jee="http://www.springframework.org/schema/jee" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/aop 
       http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
       http://www.springframework.org/schema/tx
       http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
       http://www.springframework.org/schema/security 
       http://www.springframework.org/schema/security/spring-security-2.0.1.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-2.5.xsd
       http://www.springframework.org/schema/util
       http://www.springframework.org/schema/util/spring-util-2.5.xsd
       http://www.springframework.org/schema/jee
       http://www.springframework.org/schema/jee/spring-jee-2.5.xsd">

	<!-- admon bean -->
	<bean id="userBean" class="org.jp.web.beans.admon.UserBean" scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
		<property name="unitUserBean" ref="unitUserBean" />
	</bean>

	<bean id="groupBean" class="org.jp.web.beans.admon.GroupBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
		<property name="newGroup" ref="unitGroupBean" />
	</bean>

	<bean id="unitGroupBean" class="org.jp.web.beans.admon.UnitGroupBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<bean id="unitUserBean" class="org.jp.web.beans.admon.UnitUserBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<!-- Proyect -->
	<bean id="proyectUserBean" class="org.jp.web.beans.project.ProjectBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
		<property name="beanUProyect" ref="unitProyectBean" />
	</bean>

	<bean id="unitProyectBean" class="org.jp.web.beans.project.UnitProjectBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<!-- Location -->

	<bean id="locationUserBean" class="org.jp.web.beans.location.LocationBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<bean id="treeNodeLocationBean" class="org.jp.web.beans.location.TreeNodeTest"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<!-- Survey's -->

	<bean id="surveyFormatBean" class="org.jp.web.beans.survey.SurveyFormatBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<bean id="surveyQuestionBean" class="org.jp.web.beans.survey.QuestionBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<bean id="surveyControllerBean" class="org.jp.web.beans.survey.BuildSurveyControllerBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
		<property name="unitSurveySection" ref="unitSurveySection" />
		<property name="unitQuestionBean" ref="unitQuestionBean" />
		<property name="unitPatterBean" ref="unitPatterBean" />
	</bean>
	
	<bean id="unitSurveySection" class="org.jp.web.beans.survey.UnitSurveySection"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>
	
	<bean id="unitQuestionBean" class="org.jp.web.beans.survey.UnitQuestionBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>
	
	<bean id="unitPatterBean" class="org.jp.web.beans.survey.UnitPatternBean"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>


	<bean id="buildEventSurvey" class="org.jp.web.utils.DropEventBean"
		scope="session">
		<property name="buildSurvey" ref="surveyControllerBean" />
	</bean>

	<!-- login bean -->
	<bean id="loginForm" class="org.jp.web.beans.login.LoginSecurityBean"
		scope="request">
		<property name="authenticationManager" ref="authenticationManager" />
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<!-- menu's bean -->

	<bean id="menuBean" class="org.jp.web.navigation.MenuBean" scope="session">
	</bean>


	<!-- globals -->
	<bean id="globalBean" class="org.jp.web.beans.commons.GlobalCommonsBeans"
		scope="session">
		<property name="servicemanagerBean" ref="servicesManager" />
	</bean>

	<bean id="calendarBean" class="org.jp.web.beans.commons.CalendarBean"
		scope="request">

	</bean>

	<!-- Test Bean  -->
	<bean id="testBeanDos" class="org.jp.test.MyVarGet" scope="request">
	</bean>

</beans>
