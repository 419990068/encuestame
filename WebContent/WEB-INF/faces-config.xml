<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 
 <managed-bean>
  <managed-bean-name>serviceLocateBean</managed-bean-name>
  <managed-bean-class>org.jp.web.beans.ServiceLocateBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 
 <managed-bean>
  <managed-bean-name>testBeanFaces</managed-bean-name>
  <managed-bean-class>org.jp.web.beans.TestBeanFaces</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pageTwoBean</managed-bean-name>
  <managed-bean-class>org.pao.beans.PageTwoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pageOneBean</managed-bean-name>
  <managed-bean-class>org.pao.beans.PageOneBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>servicelocatebean</property-name>
   <property-class>#{serviceLocateBean}</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 
 <navigation-rule>
  <from-view-id>/start.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>pageone</from-outcome>
   <to-view-id>/pageone.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>pagetwo</from-outcome>
   <to-view-id>/pagetwo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
